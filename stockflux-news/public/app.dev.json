{
    "devtools_port": 9091,
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing",
        "version": "10.66.39.43"
    },
    "startup_app": {
        "name": "stockflux-news",
        "uuid": "stockflux-news",
        "url": "http://localhost:8056/index.html",
        "applicationIcon": "http://localhost:8056/favicon.ico",
        "autoShow": true,
        "maxHeight": 400,
        "defaultWidth": 400,
        "minWidth": 400,
        "maxWidth": 400,
        "resizable": false,
        "frame": false,
        "contextMenu": true,
        "cornerRounding": {
            "height": 4,
            "width": 4
        },
        "customData": {
            "apiBaseUrl": "https://d3capoqa8f983r.cloudfront.net/api"
