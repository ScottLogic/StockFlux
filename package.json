{
  "name": "bitflux-openfin",
  "version": "10.1.0",
  "scripts": {
    "start": "webpack-dev-server -d --config webpack/webpack.config.dev.js",
    "openfin:dev": "node openfin-dev-launcher.js",
    "openfin:prod": "node openfin-prod-launcher.js",
    "build": "webpack -p --config webpack/webpack.config.prod.js",
    "build:deploy": "npm run build && node configJson.js",
    "serve": "http-server -p 5001",
    "prod": "npm run build && npm run serve",
    "ci": "npm run lint && npm test && npm run build",
    "lint": "eslint --plugin import jsx-ally react *.js src/ test/",
    "test": "cross-env BABEL_ENV=test mocha --opts test/mocha.opts",
    "test:watch": "npm test -- --watch",
    "test:cov": "cross-env BABEL_ENV=test babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --opts test/mocha.opts",
    "postinstall": "cd node_modules/BitFlux && npm i && grunt build:module",
    "remotedev": "remotedev --hostname=localhost --port=9090",
    "release": "npm run ci && npm version"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-template": "^6.9.0",
    "babel-types": "^6.9.1",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "grunt-cli": "^1.2.0",
    "http-server": "^0.9.0",
    "isparta": "^4.0.0",
    "json-loader": "^0.5.4",
    "jsdom": "9.3.0",
    "jsdom-global": "2.0.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.1.0",
    "remote-redux-devtools": "^0.3.3",
    "remotedev-server": "^0.1.2",
    "script-loader": "^0.7.0",
    "semver": "^5.1.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "BitFlux": "github:scottlogic/BitFlux#1.6.1",
    "babel-polyfill": "^6.3.14",
    "classnames": "^2.2.3",
    "d3fc": "^5.3.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "^1.11.1",
    "malihu-custom-scrollbar-plugin": "^3.1.3",
    "moment": "^2.13.0",
    "openfin-config-builder": "^1.0.8",
    "openfin-launcher": "^0.1.8",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.4",
    "redux": "^3.4.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.3"
  },
  "description": "Hosts the BitFlux as an OpenFin application."
}
