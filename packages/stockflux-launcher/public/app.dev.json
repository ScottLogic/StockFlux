{
    "devtools_port": 9091,
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing",
        "version": "12.69.43.22"
    },
    "startup_app": {
        "name": "stockflux-launcher",
        "uuid": "stockflux-launcher",
        "url": "http://localhost:8053/index.html",
        "applicationIcon": "http://localhost:8053/favicon.ico",
        "autoShow": true,
        "defaultHeight": 75,
        "defaultWidth": 800,
        "resizable": false,
        "maximizable": false,
        "minimizable": false,
        "frame": false,
        "shadow": true,
        "opacity": 0.95,
        "alwaysOnTop": true,
        "contextMenu": true,
        "waitForPageLoad": true,
        "cornerRounding": {
            "height": 0,
            "width": 0
        },
        "customData": {
            "apiBaseUrl": "http://localhost:3010/api",
            "isDockable": true,
            "initialDocked": true
        }
    },
    "services": [
        {
            "name": "layouts",
            "config": {
                "features": {
                    "tab": false,
                    "dock": false,
                    "snap": false
                },
                "rules": [
                    {
                        "scope": {
                            "level": "window",
                            "uuid": "stockflux-launcher",
                            "name": "search-results"
                        },
                        "config": {
                            "features": {
                                "dock": false,
                                "snap": false,
                                "tab": false
                            }
                        }
                    },
                    {
                        "scope": {
                            "level": "window",
                            "uuid": "stockflux-launcher",
                            "name": {
                                "expression": "stockflux-news.*"
                            }
                        },
                        "config": {
                            "features": {
                                "dock": true,
                                "snap": true,
                                "tab": false
                            }
                        }
                    },
                    {
                        "scope": {
                            "level": "window",
                            "uuid": "stockflux-launcher",
                            "name": {
                                "expression": "stockflux-chart.*"
                            }
                        },
                        "config": {
                            "features": {
                                "dock": true,
                                "snap": true,
                                "tab": false
                            }
                        }
                    },
                    {
                        "scope": {
                            "level": "window",
                            "uuid": "stockflux-launcher",
                            "name": {
                                "expression": "stockflux-watchlist.*"
                            }
                        },
                        "config": {
                            "features": {
                                "dock": true,
                                "snap": true,
                                "tab": false
                            }
                        }
                    }
                ]
            }
        }
    ],
    "shortcut": {
        "company": "ScottLogic",
        "description": "Hosts StockFlux App Launcher as an OpenFin application.",
        "name": "StockFlux App Launcher",
        "icon": "http://localhost:8053/favicon.ico"
    },
    "splashScreenImage": "http://localhost:8053/splashscreen.png"
}